{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## 📝 Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## 🔧 Maintenance",
      "labels": ["chore", "dependencies"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test", "tests"]
    }
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n**Full Changelog**: ${{RELEASE_DIFF}}\n",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "flags": "gu"
    },
    {
      "pattern": "\\[([^\\]]+)\\]",
      "target": "$1",
      "flags": "gu"
    }
  ],
  "duplicate_filter": {
    "pattern": "\\[([^\\]]+)\\]",
    "flags": "gu"
  },
  "transformers": [
    {
      "pattern": "[\\-\\*] (?:\\[(...|fix|feat|docs|style|refactor|test|chore|perf|ci|build|revert)(?:\\([^\\)]*\\))?\\])? ?(.*)",
      "target": "- $2"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [
    "Owner/main"
  ],
  "tag_resolver": {
    "method": "semver",
    "filter": {
      "pattern": "v?(.*)",
      "flags": "gu"
    }
  },
  "base_branches": [
    "master",
    "main"
  ]
}